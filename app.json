{
  "expo": {
    "name": "Shopping Cartulator",
    "slug": "shopping-cartulator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A handy little shopping list app that keeps a running total value of items added to your cart.",
    "githubUrl": "https://github.com/dcjwest/shopping-cartulator"
  }
}
